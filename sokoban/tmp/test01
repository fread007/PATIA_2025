(define (problem sokoban-scoria---level-0)
  (:domain sokoban)
  (:objects 
     l0-0 l0-1 l1-0 l1-1 l2-0 l2-1  - lieu)
  (:init 
        (connectD l0-0 l1-0)
        (connectR l0-0 l0-1)
        (Pon l0-0)
        (connectL l0-1 l0-0)
        (connectD l0-1 l1-1)
        (clear l0-1)
        (connectU l1-0 l0-0)
        (connectD l1-0 l2-0)
        (connectR l1-0 l1-1)
        (Con l1-0)
        (connectU l1-1 l0-1)
        (connectL l1-1 l1-0)
        (connectD l1-1 l2-1)
        (Con l1-1)
        (isG l1-1)
        (connectU l2-0 l1-0)
        (connectR l2-0 l2-1)
        (isG l2-0)
        (clear l2-0)
        (connectU l2-1 l1-1)
        (connectL l2-1 l2-0)
        (clear l2-1)
  )
  (:goal 
    (and 
         (Con l1-0)
         (Con l1-1)
    ))
  )
